function somaMatriz(matriz) {
    let soma = 0;
    for (let linha of matriz) {
        for (let num of linha) {
            soma += num;
        }
    }
    return soma;
}

// Exemplo de uso:
let matrizExemplo = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
];
console.log(somaMatriz(matrizExemplo));
